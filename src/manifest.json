{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "default_locale": "en",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png",
    "48": "images/icon48.png"
  },
  "description": "Replaces the start page with your bookmarks.",
  "manifest_version": 2,
  "minimum_chrome_version": "18",
  "name": "Favilicous",
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "homepage_url": "https://github.com/wummel/favilicous/",
  "version": "0.9",
  "web_accessible_resources": [
    "newtab.html"
  ],
  "content_security_policy":
    "object-src 'none'; script-src 'self'; img-src 'self' https://favicons.githubusercontent.com"
}
