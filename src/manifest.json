   "chrome_url_overrides": {
      "newtab": "newtab.html"
   },
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "name": "Favilicous",
   "permissions": ["bookmarks", "chrome://favicon/", "tabs", "contextMenus"],
